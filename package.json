{
  "name": "interview-prep",
  "version": "1.0.0",
  "description": "Gotta remember all the crap",
  "type": "module",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint src tests",
    "format": "prettier . --write",
    "lc-count": "ls ./src -1 | wc -l",
    "lc-book": "node ./bin/lcBook.js",
    "lc-test": "./bin/lcTest.sh",
    "lc-today": "ls ./src -lt --time=birth --time-style='+%b %-d' | grep \"$(date '+%b %-d')\" | wc -l",
    "lc-touch": "node ./bin/lcTouch.js",
    "lc-uglify": "./bin/lcUglify.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beakerandjake/interview-prep.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beakerandjake/interview-prep/issues"
  },
  "homepage": "https://github.com/beakerandjake/interview-prep#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "html-to-text": "^9.0.5",
    "jest": "^29.4.3",
    "jest-extended": "^4.0.2",
    "leetcode-query": "^0.2.7",
    "prettier": "2.8.4",
    "uglify-js": "^3.17.4"
  }
}
